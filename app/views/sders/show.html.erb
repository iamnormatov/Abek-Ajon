
<%= render @sder %>

<div>
  <%= link_to "Edit", edit_sder_path(@sder), class:"button-25" %> 
  <%= link_to "Back", project_sders_path(@project), class:"button-26" %>
  <%= button_to "Destroy", @sder, method: :delete, class:"button-24"  %>

</div>




<div>
<%= render "sends/form", send: @send %>
</div>

<div>
<% @sends.each do |send| %>
  <p align="center"><%= send.title %>
  <% if send.id %>
    
  <%= link_to send, "data-turbo-method": :delete do %>
<i class="bi bi-trash3"></i>
  
  <% end %>

  <% end %>
  </p>
<% end %>

</div>
</div>

